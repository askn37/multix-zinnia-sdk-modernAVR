#------------------------------------------------------------
# AVR-EB family common values (NVMCTRL version 5)
#------------------------------------------------------------

part
    desc                   = "AVR-EB family common values";
    id                     = ".avreb";
    family_id              = "AVR    ";
    prog_modes             = PM_SPM | PM_UPDI;
    n_boot_sections        = 1;
    boot_section_size      = 256;
    # Shared UPDI pin, HV on _RESET
    hvupdi_variant         = 2;
    nvm_base               = 0x1000;
    ocd_base               = 0x0f80;
    idr                    = 0x32;  # Passing hvupdi_variant in jtagmkII

    memory "boot"
        # Space up to BOOTSIZE(BOOTEND)
        size               = 0;
        readsize           = 256;
    ;

    memory "application"
        # Space between BOOTSIZE and CODESIZE
        size               = 0;
        readsize           = 256;
    ;

    memory "flash"
        # size will be defined later
        page_size          = 64;
        offset             = 0x800000;
        readsize           = 256;
    ;

    memory "bootrow"
        size               = 64;
        page_size          = 64;
        offset             = 0x1100;
        readsize           = 64;
    ;

    memory "userrow"
        # size will be defined later
        size               = 64;
        page_size          = 64;
        offset             = 0x1200;
        readsize           = 64;
    ;

    memory "usersig"
        alias "userrow";
    ;

    memory "eeprom"
        size               = 512;
        page_size          = 2;
        offset             = 0x1400;
        readsize           = 256;
    ;

    memory "data"
        # SRAM, only used to supply the offset
        # This is a fake address that does not exist in ATDF in the UPDI implementation
        offset             = 0x01000000;
    ;

    memory "io"
        size               = 4160;
        readsize           = 1;
    ;

    # FUSE members

    memory "lock"
        size               = 4;
        offset             = 0x1040;
        initval            = 0x5cc5c55c;
        readsize           = 4;
    ;

    memory "fuse0"
        size               = 1;
        initval            = 0x00;
        offset             = 0x1050;
        readsize           = 1;
    ;

    memory "wdtcfg"
        alias "fuse0";
    ;

    memory "fuse1"
        size               = 1;
        initval            = 0x00;
        offset             = 0x1051;
        readsize           = 1;
    ;

    memory "bodcfg"
        alias "fuse1";
    ;

    memory "fuse2"
        size               = 1;
        initval            = 0x00;
        bitmask            = 0x07;
        offset             = 0x1052;
        readsize           = 1;
    ;

    memory "osccfg"
        alias "fuse2";
    ;

    memory "fuse5"
        size               = 1;
        initval            = 0xc0;
        bitmask            = 0xed;
        offset             = 0x1055;
        readsize           = 1;
    ;

    memory "syscfg0"
        alias "fuse5";
    ;

    memory "fuse6"
        size               = 1;
        initval            = 0x08;
        bitmask            = 0x1f;
        offset             = 0x1056;
        readsize           = 1;
    ;

    memory "syscfg1"
        alias "fuse6";
    ;

    memory "fuse7"
        size               = 1;
        initval            = 0x00;
        offset             = 0x1057;
        readsize           = 1;
    ;

    memory "codesize"
        alias "fuse7";
    ;

    memory "append"
        alias "fuse7";
    ;

    memory "fuse8"
        size               = 1;
        initval            = 0x00;
        offset             = 0x1058;
        readsize           = 1;
    ;

    memory "bootsize"
        alias "fuse8";
    ;

    memory "bootend"
        alias "fuse8";
    ;

    memory "fusea"
        size               = 2;
        initval            = 0x03;
        bitmask            = 0xfff3;
        offset             = 0x105a;
        readsize           = 1;
    ;

    memory "pdicfg"
        alias "fusea";
    ;

    # *** CAUTION ***
    # The 'fuses' of the AVR_EB series is originally 11 bytes, 
    # but is limited to 9 bytes here to avoid irrecoverable failures. 
    # To overwrite the last two bytes added, 
    # explicitly specify 'fusea' or 'pdicfg'.
    memory "fuses"
        size               = 9;
        page_size          = 16;
        offset             = 0x1050;
        readsize           = 16;
    ;

    # PRODUCTION_SIGNATURE members

    memory "signature"
        size               = 3;
        offset             = 0x1080;
        readsize           = 3;
    ;

    memory "prodsig"
        size               = 125;
        page_size          = 125;
        offset             = 0x1083;
        readsize           = 125;
    ;

    memory "tempsense"
        size               = 2;
        offset             = 0x1084;
        readsize           = 1;
    ;

    memory "sernum"
        size               = 16;
        offset             = 0x1090;
        readsize           = 1;
    ;

    memory "sib"
        # This cannot be read as normal memory
        # Can only be used with UPDI ASI physical layer
        size               = 32;
        readsize           = 1;
    ;
;

