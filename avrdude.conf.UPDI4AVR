#------------------------------------------------------------
# AVR-DA family common values (NVMCTRL version 2)
#------------------------------------------------------------

part
    desc                   = "AVR-DA/DB family common values";
    id                     = ".avrdx";
    family_id              = "AVR    ";
    prog_modes             = PM_SPM | PM_UPDI;
    n_boot_sections        = 1;
    boot_section_size      = 512;
    # Dedicated UPDI pin, no HV
    hvupdi_variant         = 1;
    nvm_base               = 0x1000;
    ocd_base               = 0x0f80;
    idr                    = 0x31;  # substitute hvupdi_variant

    memory "boot"
        # Space up to BOOTSIZE(BOOTEND)
        size               = 0;
        readsize           = 256;
    ;

    memory "application"
        # Space between BOOTSIZE and CODESIZE
        size               = 0;
        readsize           = 256;
    ;

    memory "flash"
        # size will be defined later
        page_size          = 512;
        offset             = 0x800000;
        readsize           = 256;
    ;

    memory "userrow"
        # size will be defined later
        size               = 32;
        page_size          = 32;
        offset             = 0x1080;
        readsize           = 32;
    ;

    memory "usersig"
        alias "userrow";
    ;

    memory "eeprom"
        size               = 512;
        page_size          = 2;
        offset             = 0x1400;
        readsize           = 256;
    ;

    memory "data"
        # SRAM, only used to supply the offset
        # This is a fake address that does not exist in ATDF in the UPDI implementation
        offset             = 0x01000000;
    ;

    memory "io"
        size               = 4160;
        readsize           = 1;
    ;

